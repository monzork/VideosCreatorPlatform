{
  "entryFile": "app/server.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["app/**/*controller.ts"],
  "spec": {
    "basePath": "/v1",
    "outputDirectory": "build",
    "specVersion": 3
  },
  "routes": {
    "basePath": "/v1",
    "entryFile": "./app/server.ts",
    "routesDir": "./build",
    "authenticationModule": "./app/middlewares/auth.ts"
  }
}
